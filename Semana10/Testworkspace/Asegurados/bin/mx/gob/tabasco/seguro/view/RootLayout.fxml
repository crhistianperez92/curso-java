<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mx.gob.tabasco.seguro.view.RootLayoutController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Asegurado">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handleAbrirAsegurado" text="Abrir" />
                  <MenuItem mnemonicParsing="false" onAction="#handleAgregarAsegurado" text="Agregar" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="?">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handleAyuda" text="Ayuda" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
